{
  "name": "muralla-frontend",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "^20.19.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.2.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "vite build",
    "lint": "eslint .",
    "lint:i18n": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "validate-translations": "node scripts/validate-translations.js",
    "i18n:check": "npm run validate-translations",
    "i18n:fix": "npm run validate-translations && npm run lint:i18n",
    "precommit": "npm run lint && npm run validate-translations",
    "pre-push": "npm run i18n:check"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@mercadopago/sdk-js": "^0.0.3",
    "@types/react-window": "^1.8.8",
    "@types/socket.io-client": "^3.0.0",
    "axios": "^1.11.0",
    "csv-parser": "^3.2.0",
    "dexie": "^4.2.0",
    "i18next": "^25.3.6",
    "lucide-react": "^0.541.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1",
    "react-window": "^1.8.11",
    "socket.io-client": "^4.8.1",
    "xlsx-populate": "^1.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "framer-motion": "^12.23.12",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.7.1",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
